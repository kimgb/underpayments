%tr
  %td= "Incomplete"
  %td= user.full_name
  %td{ class: "hidden-xs" }= user.employer && user.employer.name
  %td{ class: "hidden-xs" }= number_to_currency(user.claim && user.claim.hourly_pay)
  %td{ class: "hidden-xs" }= (user.claim && user.claim.hours_worked || 0.0).round
  %td{ class: "hidden-xs" }= number_to_currency(user.claim && user.claim.lost_wages)
  %td= link_to '', admin_user_path(user), class: "btn btn-sm btn-default glyphicon glyphicon-eye-open pull-right"
