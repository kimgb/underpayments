%table.table
  %thead
    %tr
      %th Sections complete
      %th Name
      %th Email
      %th Mobile?
      %th

  %tbody#user-list
    - if @users.any?
      - @users.each do |user|
        - if user.profile.present?
          = render partial: "admin/users/#{@view_context}_row", locals: { user: user }
    - else
      %tr
        %td{ colspan: 5 }
          No claims to display
